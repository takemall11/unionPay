{
  "name": "nahuomall/unionpay",
  "description": "Union Pay API SDK for PHP",
  "type": "library",
  "keywords": ["union", "sdk", "hyperf"],
  "require": {
    "php": ">=8.0",
    "hyperf/framework": "~3.1.0",
    "hyperf/guzzle": "~3.1.0",
    "hyperf/constants": "~3.1.0",
    "hyperf/server": "~3.1.0",
    "hyperf/exception-handler": "~3.1.0",
    "ext-simplexml": "*",
    "spatie/array-to-xml": "^3.2",
    "mtownsend/xml-to-array": "^2.0",
    "ext-curl": "*",
    "ext-openssl": "*"
  },
  "support": {
    "email": "yangyang@nahuomall.com",
    "issues": "https://github.com/nahuoMall/mideaPay",
    "docs": "https://open.jxhh.com"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "hyperf/devtool": "~3.1.0",
    "hyperf/testing": "~3.1.0",
    "mockery/mockery": "^1.0",
    "phpstan/phpstan": "^1.0",
    "swoole/ide-helper": "^5.0"
  },
  "provides": [
    "UnionPay\\Api\\Provider\\UnionPayServiceProvider"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "since",
      "email": "yangyang@nahuomall.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "UnionPay\\Api\\": "src/"
    }
  },
  "scripts": {
    "cs-fix": "php-cs-fixer fix src"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "hyperf": {
      "config": "UnionPay\\Api\\ConfigProvider"
    }
  }
}
